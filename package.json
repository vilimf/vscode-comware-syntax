{
  "name": "vscode-comware-syntax",
  "displayName": "HPE/H3C Comware Config Syntax",
  "description": "Syntax highlighting for HPE/H3C Comware switch configurations",
  "icon": "icon.png",
  "version": "0.1",
  "publisher": "vilimf",
  "repository": {
    "type": "git",
    "url": "https://github.com/vilimf/vscode-comware-syntax.git"
  },
  "engines": {
    "vscode": "^1.24.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "hpe",
        "aliases": [
          "HPE comware",
          "hpe",
          "h3c"
        ],
        "extensions": [
          ".config",
          ".comware",
          ".cfg"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hp",
        "scopeName": "text.comware",
        "path": "./syntaxes/hpe.tmLanguage.json"
      }
    ]
  }
}
